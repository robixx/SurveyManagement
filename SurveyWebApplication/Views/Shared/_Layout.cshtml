<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/toastr.min.css" rel="stylesheet" />
    <script src="~/Content/toastr.min.js"></script>
    <link href="~/Content/CustomStyle.css" rel="stylesheet" />   
    <link href="~/Content/EasyUi/easyui.css" rel="stylesheet" />
    <link href="~/Content/EasyUi/icon.css" rel="stylesheet" />
    <link href="~/Content/color.css" rel="stylesheet" />
   
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Content/bootstrap.bundle.min.js"></script>
    <script src="~/Content/EasyUi/jquery.min.js"></script>
    <script src="~/Content/EasyUi/jquery.easyui.min.js"></script>
 
   
    <style>
        .background {
            background: -webkit-gradient(linear,left top,left bottom,color-stop(1,#1f4b65),color-stop(0,#83b7b7));
            border-color: black;
            border-radius: 2px;
            color: white !important;
        }

        .navbar-nav .nav-link.active, .navbar-nav .nav-link.show {
            color: white !important;
        }
        #headcolor {
            color: white !important;
        }
      
    </style>

</head>
<body>   

    <nav class="navbar background navbar-expand-md navbar-light bg-light mb-4">
        <div class="container">
            <a class="navbar-brand" id="headcolor" href="#">SurveyApplication</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarCollapse">
                <ul class="navbar-nav me-auto mb-2 mb-md-0">
                    <li class="nav-item">
                        <a class="nav-link active" href="@Url.Action("OrganizationList", "Organization")">Organized</a>
                    </li>
                    
                    <li class="nav-item">
                        <a class="nav-link active" href="@Url.Action("QuestionSet", "QuestionSets")">SurveySet</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" href="@Url.Action("OptionTypeList", "OptionType")">OptionType</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" href="@Url.Action("QuestionTitleList", "Question")">SurveyTitle</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" href="@Url.Action("SetManageList", "SetManage")">SetManage</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" href="@Url.Action("QuestionCreate", "QuestionManage")">SurveyCreate</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" href="@Url.Action("OrganizationSurvey", "SurveyManage")">Survey</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" href="@Url.Action("Test", "SurveyManage")">SurveyResult</a>
                    </li>
                </ul>
                <form class="d-flex" id="headcolor">
                    <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-success" type="submit">Search</button>
                </form>
            </div>
        </div>
    </nav>




    <div class="container body-content">
        @RenderBody()
     
    </div>
      
    <footer class="footer mt-auto py-3 bg-light fixed-bottom">
        <div class="container">
            <p>&copy; @DateTime.Now.Year - Developed By  <a href="https://mediasoftbd.com/" style="text-decoration:none !important">MediaSoft Data System Limited.</a></p>
        </div>
    </footer>
    
   
</body>
</html>
