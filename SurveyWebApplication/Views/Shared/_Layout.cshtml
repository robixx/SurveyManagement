<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/toastr.min.css" rel="stylesheet" />
    <script src="~/Content/toastr.min.js"></script>
    <link href="~/Content/CustomStyle.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/EasyUi/easyui.css" rel="stylesheet" />
    <link href="~/Content/EasyUi/icon.css" rel="stylesheet" />
    <link href="~/Content/color.css" rel="stylesheet" />
    <link href="~/Content/EasyUi/demo.css" rel="stylesheet" />
    <script src="~/Content/EasyUi/jquery.min.js"></script>
    <script src="~/Content/EasyUi/jquery.easyui.min.js"></script>
    
   
    <style>
        #background {
            background: -webkit-gradient(linear,left top,left bottom,color-stop(0,#085454),color-stop(1,#325da7));
            border-color: black;
            border-radius: 2px;
            color:white !important;
            
        }
    </style>

</head>
<body>
   

    <div class="container">
        <header class="d-flex flex-wrap align-items-center justify-content-center justify-content-md-between py-1 mb-3 border-bottom">
            <a href="/" class="d-flex align-items-center col-md-3 mb-2 mb-md-0 text-dark text-decoration-none">
                <svg class="bi me-2" width="40" height="32" role="img" aria-label="Bootstrap"><use xlink:href="#bootstrap" /></svg>
            </a>

            <ul class="nav col-12 col-md-auto mb-2 justify-content-center mb-md-0">
               
                <li><a href="@Url.Action("QuestionSet", "QuestionSets")" class="nav-link px-2 link-dark">QuestionSet</a></li>               
                <li><a href="@Url.Action("OptionTypeList", "OptionType")" class="nav-link px-2 link-dark">OptionTypeList</a></li>
                <li><a href="@Url.Action("SetManageList", "SetManage")" class="nav-link px-2 link-dark">SetManage</a></li>
                <li><a href="@Url.Action("QuestionTitleList", "Question")" class="nav-link px-2 link-dark">Question</a></li>
                <li><a href="@Url.Action("QuestionCreate", "QuestionManage")" class="nav-link px-2 link-dark">QuestionManage</a></li>
                <li><a href="@Url.Action("SetList", "SurveyManage")" class="nav-link px-2 link-dark">Set</a></li>
                @*<li><a href="@Url.Action("SurveyQuestion", "SurveyManage")" class="nav-link px-2 link-dark">SurveyQuestion</a></li>*@
                
            </ul>

            <div class="col-md-3 text-end">
                <button type="button" class="btn btn-outline-primary me-2">Login</button>
                <button type="button" class="btn btn-primary">Sign-up</button>
            </div>
        </header>
    </div>




    <div class="container body-content">
        @RenderBody()
        <hr />
    </div>
      
    <footer class="footer mt-auto py-3 bg-light fixed-bottom">
        <div class="container">
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </div>
    </footer>
    
   
</body>
</html>
